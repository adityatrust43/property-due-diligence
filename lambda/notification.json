{
    "LambdaFunctionConfigurations": [
        {
            "Id": "document-analysis-trigger",
            "LambdaFunctionArn": "arn:aws:lambda:us-east-1:530349877694:function:document-analysis-function",
            "Events": ["s3:ObjectCreated:*"],
            "Filter": {
                "Key": {
                    "FilterRules": [
                        {
                            "Name": "prefix",
                            "Value": "uploads/"
                        },
                        {
                            "Name": "suffix",
                            "Value": ".pdf"
                        }
                    ]
                }
            }
        }
    ]
}
